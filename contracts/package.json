{
  "name": "@prb/hardhat-template",
  "description": "Hardhat-based template for developing Solidity smart contracts",
  "version": "1.0.0",
  "author": {
    "name": "Paul Razvan Berg",
    "url": "https://github.com/paulrberg"
  },
  "scripts": {
    "node": "hardhat node --network hardhat",
    "clean": "shx rm -rf ./artifacts ./cache ./coverage ./types ./coverage.json && yarn typechain",
    "commit": "git-cz",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"test/**/*.ts\" && yarn typechain",
    "deploy": "yarn deploy:apps && yarn deploy:spaces && yarn deploy:accounts",
    "deploy:local": "yarn deploy:apps:local && yarn deploy:accounts:local",
    "deploy:chain": "yarn deploy:apps:chain && yarn deploy:accounts:chain",
    "deploy:apps:local": "hardhat deploy:EmbraceApps --network localhost",
    "deploy:apps:chain": "source .env && hardhat deploy:EmbraceApps --network $HARDHAT_NETWORK",
    "deploy:spaces:local": "hardhat deploy:EmbraceSpaces --accountsaddress {CONTRACT_ACCRESS_OF_ACCOUNTS} --network localhost",
    "deploy:spaces:chain": "source .env && hardhat deploy:EmbraceSpaces --accountsaddress {CONTRACT_ACCRESS_OF_ACCOUNTS} --network $HARDHAT_NETWORK",
    "deploy:accounts:local": "hardhat deploy:EmbraceAccounts --network localhost",
    "deploy:accounts:chain": "source .env && hardhat deploy:EmbraceAccounts --network $HARDHAT_NETWORK",
    "deploy:appSocials:local": "source .env && hardhat deploy:AppSocials --network localhost",
    "deploy:appSocials:chain": "source .env && hardhat deploy:AppSocials --network $HARDHAT_NETWORK",
    "cp:artifacts": "cp ./artifacts/contracts/EmbraceApps.sol/EmbraceApps.json ./artifacts/contracts/EmbraceSpaces.sol/EmbraceSpaces.json ./artifacts/contracts/EmbraceAccounts.sol/EmbraceAccounts.json ../frontend/data/contractArtifacts/",
    "cp:appArtifacts": "cp ./artifacts/contracts/app/AppCreations.sol/AppCreations.json ./artifacts/contracts/app/AppCreationsCollection.sol/AppCreationsCollection.json ./artifacts/contracts/app/AppSocials.sol/AppSocials.json ../frontend/data/contractArtifacts/",
    "cp:types": "cp ./types/contracts/*.ts ../frontend/data/contractTypes/",
    "cp:appTypes": "cp ./types/contracts/app/*.ts ../frontend/data/contractTypes/app",
    "deploy:all": "hh deploy:EmbraceAll --network localhost",
    "deploy:oneStep": "hh compile && hh typechain && yarn deploy:all && yarn cp:artifacts && yarn cp:appArtifacts && yarn cp:types && yarn cp:appTypes",
    "deploy:oneStepMumbai": "hh compile && hh typechain && hh deploy:EmbraceAll --network polygonMumbai && yarn cp:artifacts && yarn cp:appArtifacts && yarn cp:types && yarn cp:appTypes",
    "deploy:oneStepGoerli": "hh compile && hh typechain && hh deploy:EmbraceAll --network goerli && yarn cp:artifacts && yarn cp:appArtifacts && yarn cp:types && yarn cp:appTypes",
    "deploy:communities": "hh compile && hh typechain && hh deploy:EmbraceCommunities --network polygonMumbai --accountsaddress 0xDc64a140Aa3E981100a9becA4E685f962f0cF6C9",
    "getSpaces": "ts-node scripts/getSpaces.ts {CONTRACT_ADDRESS} {NETWORK?}",
    "createSpace": "ts-node scripts/createSpace.ts {CONTRACT_ADDRESS} {SPACE_HANDLE} {METADATA} {NETWORK?}",
    "createApp": "ts-node scripts/createApp.ts {CONTRACT_ADDRESS} {APP_CODE} {APP_ADDRESS} {APP_METADATA} {NETWORK?}",
    "updateAppMetadata": "ts-node scripts/updateAppMetadata.ts {CONTRACT_ADDRESS} {APP_INDEX} {APP_METADATA} {NETWORK?}",
    "getApps": "ts-node scripts/getApps {CONTRACT_ADDRESS}",
    "lint": "yarn lint:sol && yarn lint:ts && yarn prettier:check",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --config ./.eslintrc.yml --ignore-path ./.eslintignore --ext .js,.ts .",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --config ./.prettierrc.yml --write \"**/*.{js,json,md,sol,ts,yml}\"",
    "prettier:check": "prettier --check --config ./.prettierrc.yml \"**/*.{js,json,md,sol,ts,yml}\"",
    "test": "hardhat test",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.1",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.1",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.3",
    "@types/chai": "^4.3.3",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.8.5",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "chai": "^4.3.6",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "ethers": "^5.7.1",
    "fs-extra": "^10.1.0",
    "hardhat": "^2.12.0",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-beta.24",
    "shx": "^0.3.4",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.2",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.8.4",
    "web3.storage": "^4.4.0"
  },
  "files": [
    "/contracts"
  ],
  "keywords": [
    "blockchain",
    "ethers",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity",
    "template",
    "typescript",
    "typechain"
  ],
  "packageManager": "yarn@3.2.1",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.8.0",
    "@tableland/evm": "^4.0.0",
    "@tableland/local": "^1.0.0-pre.1"
  }
}
